  <h2>Find a Pet:</h2>
  <%= form_tag "/pets/search", id: "js-search" do %> <br>
    <%= label_tag "pet[name]", "By pet's name:" %> <br>
    <%= text_field_tag "pet[name]" %> <br><br>
    <!--<%= label_tag "pet[species]", "Species:" %>
    <%= select_tag "pet[species]", options_for_select(["Dog", "Cat"]) %> <br><br>-->
    <%= label_tag "pet[owner_first_name]", "By owner's name:" %> <br>
    <%= text_field_tag "pet[owner_first_name]", nil, placeholder: "first" %>
    <!--<%= label_tag "pet[owner_last_name]", "Owner's Last Name:" %> <br>-->
    <%= text_field_tag "pet[owner_last_name]", nil, placeholder: "last" %> <br> <br>
    <%= submit_tag "Find Pet", id: "js-submitSearch" %> 
    <%= link_to "Cancel", "#", class: "js-cancel" %> <br> <br>
  <% end %>
  <div id="js-searchResults"></div>