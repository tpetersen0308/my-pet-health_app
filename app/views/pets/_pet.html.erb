<h4><%= pet.name %></h4>
<p><%= link_to "View #{pet.name}'s screenings", pet_health_screenings_path(pet) %></p>
<ul></ul>
  <li>Type: <%= pet.sex %> <%= pet.species %></li>
  <li>Owner: <%= pet.owner.name %></li>
  <li>Age: <%= pet.age %></li>
  <li>Veterinarian(s):
    <ul>
      <% pet.veterinarians.each do |vet| %>
        <li><%= link_to vet.name, user_path(vet) %></li>
      <% end %>
    </ul>
  </li>
</ul>